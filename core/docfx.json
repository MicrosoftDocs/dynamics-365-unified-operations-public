*{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.gif",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "layout": "Conceptual",
      "breadcrumb_path": "/breadcrumb/toc.yml"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "d365OpsCore"
  }
}

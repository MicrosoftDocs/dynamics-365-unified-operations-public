{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "PULL_REQUEST_TEMPLATE.md",
          "ISSUE_TEMPLATE.md",
          "**/_themes/**",
          "**/_themes.pdf/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/**.png",
          "**/**.jpg",
          "**/**.jpeg",
          "**/**.gif",
          "**/**.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/dynamics-365-unified-operations-public",
      "feedback_product_url": "https://ideas.dynamics.com",
      "contributors_to_exclude": [
        "OpenLocalizationService",
        "buck1ey",
        "bishalgoswami"
      ],
      "layout": "Conceptual",
      "searchScope": [
        "Dynamics 365",
        "Unified Operations"
      ],
      "extendBreadcrumb": "true",
      "breadcrumb_path": "/dynamics365/unified-operations/breadcrumb/toc.json",
      "uhfHeaderId": "MSDocsHeader-Dynamics365",
      "brand": "dyn-ops",
      "_op_documentIdPathDepotMapping": {
        "dev-itpro/": {
          "folder_relative_path_in_docset": ".",
          "depot_name": "MSDN.d365OpsDevITPro"
        },
        "financials/": {
          "folder_relative_path_in_docset": ".",
          "depot_name": "MSDN.d365OpsFinancials"
        },
        "fin-and-ops/": {
          "folder_relative_path_in_docset": ".",
          "depot_name": "MSDN.d365OpsCore"
        },
        "retail/": {
          "folder_relative_path_in_docset": ".",
          "depot_name": "MSDN.d365OpsRetail"
        },
        "supply-chain/": {
          "folder_relative_path_in_docset": ".",
          "depot_name": "MSDN.d365OpsSCM"
        },
        "talent/": {
          "folder_relative_path_in_docset": ".",
          "depot_name": "MSDN.d365OpsHR"
        }
      }
    },
    "fileMetadata": {
      "featureFlags": {
        "dev-itpro/**/*.md": [
          "show_learn_banner"
        ],
        "fin-and-ops/**/*.md": [
          "show_learn_banner"
        ],
        "financials/**/*.md": [
          "show_learn_banner"
        ],
        "supply-chain/**/*.md": [
          "show_learn_banner"
        ]
      },
      "learn_banner_products": {
        "dev-itpro/**/*.md": [
          "dynamics-finance-operations"
        ],
        "fin-and-ops/**/*.md": [
          "dynamics-finance-operations"
        ],
        "financials/**/*.md": [
          "dynamics-finance-operations"
        ],
        "supply-chain/**/*.md": [
          "dynamics-finance-operations"
        ]
      },
        "feedback_product_url": {
          "retail/**/*.md": "https://ideas.dynamics.com"
        },
       "searchScope": {
          "talent/**.md": [
            "Dynamics 365",
            "Unified Operations",
            "Talent"
        ],
          "retail/**/**.md": [
            "Dynamics 365",
            "Unified Operations",
            "Retail"
        ]
       },
      "breadcrumb_path": {
        "dev-itpro/lifecycle-services/ax-2012/**/*.md": "/dynamics365/unified-operations/breadcrumb/toc.json"}
    },
    "template": [],
    "dest": "d365F-O",
    "markdownEngineName": "markdig"
  }
}
